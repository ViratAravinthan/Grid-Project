  private void Button_Click(object sender, RoutedEventArgs e)
        {
            if (txtname.Text != "" && txtmail.Text != "" && pwdpass.Password != "" && rbmale.IsChecked == true || rbfmale.IsChecked == true)
            {
                MessageBox.Show("SucessFully Registered","Register",MessageBoxButton.OK,MessageBoxImage.Information);
            }
            else
            {
                MessageBox.Show("Fill All Details", "Register", MessageBoxButton.OK, MessageBoxImage.Error);
            }
                
        }
    }
}
